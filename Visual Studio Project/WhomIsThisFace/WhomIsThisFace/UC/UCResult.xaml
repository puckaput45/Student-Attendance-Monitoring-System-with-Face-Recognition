<UserControl x:Class="WhomIsThisFace.UC.UCResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             >
    <Grid>
        <Button x:Name="btnOpenLocalSheet"  Background="#FF3AB79F" BorderBrush="#FF3AB776" Margin="90,30,90,0" Content="Open Local Sheet" VerticalAlignment="Top" Click="btnOpenLocalSheet_Click"  />

        <ComboBox VerticalAlignment="Top" Width="120" x:Name="cbResultDate" SelectionChanged="cbResultDate_SelectionChanged"/>
        <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="0,104,0,0">
            <StackPanel x:Name="StackPanelMain"/>
        </ScrollViewer>
        <Button x:Name="btnSaveLocalSheet"  Background="#FF3AB79F" BorderBrush="#FF3AB776" Margin="90,67,90,0" Content="Save Local Sheet" VerticalAlignment="Top" Click="btnSaveLocalSheet_Click"  />
    </Grid>
</UserControl>
